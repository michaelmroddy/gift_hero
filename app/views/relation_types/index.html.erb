<div class="page-header">
  <h1>
    Relation types
    <a href="/relation_types/new" class="btn btn-lg btn-success">New Relation type</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Actions</th>
      </tr>

      <% @relation_types.each do |relation_type| %>
      <tr>
        <td>
          <a href="/relation_types/<%= relation_type.id %>" class="btn btn-primary">Show</a>
          <a href="/relation_types/<%= relation_type.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_relation_type/<%= relation_type.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @relation_types, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
